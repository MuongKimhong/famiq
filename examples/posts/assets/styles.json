{
  "#container": {
    "justify_content": "center",
    "align_items": "center",
    "width": "90%",
    "height": "90%",
    "margin": "auto auto auto auto"
  },
  ".username": {
    "color": "srgba 0.118, 0.302, 0.639, 1.0",
    "font_size": "25"
  },
  ".title": {
    "color": "srgba 0.0, 0.0, 0.0, 1.0"
  },
  ".title-wrapper": {
    "border": "0px 0px 0px 0px",
    "margin_top": "5px",
    "margin_bottom": "5px"
  },
  ".image": {
    "margin_top": "5px",
    "border": "1px 1px 1px 1px",
    "border_radius": "5px 5px 5px 5px"
  },
  ".post-container": {
    "border_color": "srgba 0.929, 0.929, 0.929, 1.0",
    "background_color": "srgba 0.929, 0.929, 0.929, 1.0",
    "margin_top": "5px",
    "margin_bottom": "5px"
  },
  ".like-txt": {
    "color": "srgba 0.0, 0.0, 0.0, 1.0",
    "font_size": "28"
  },
  ".like-btn": {
    "margin_left": "10px",
    "margin_top": "5px"
  },
  ".action-container": {
    "margin_top": "15px",
    "flex_direction": "row"
  }
}
